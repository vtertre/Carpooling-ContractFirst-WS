<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:hr="http://iaws/ws/contractfirst/covoiturage" elementFormDefault="qualified"
	targetNamespace="http://iaws/ws/contractfirst/covoiturage">
	
	<xs:element name="InscriptionRequest" type="exem:InscriptionRequestType"
		xmlns:exem="http://iaws/ws/contractfirst/covoiturage" />
		
	<xs:complexType name="InscriptionRequestType">
		<xs:all>
			<xs:element type="exem:IdentiteType" name="Identite"
				xmlns:exem="http://iaws/ws/contractfirst/covoiturage" />
			<xs:element type="exem:MailType" name="Mail"
				xmlns:exem="http://iaws/ws/contractfirst/covoiturage" />
			<xs:element type="exem:AdressePostaleType" name="AdressePostale"
				xmlns:exem="http://iaws/ws/contractfirst/covoiturage" />
		</xs:all>
	</xs:complexType>

	<xs:complexType name="IdentiteType">
		<xs:all>
			<xs:element type="xs:string" name="Nom" />
			<xs:element type="xs:string" name="Prenom" />
		</xs:all>
	</xs:complexType>

	<xs:complexType name="MailType">
		<xs:all>
			<xs:element type="xs:string" name="Perso" />
			<xs:element type="xs:string" name="Domaine" />
		</xs:all>
	</xs:complexType>

	<xs:complexType name="AdressePostaleType">
		<xs:all>
			<xs:element type="xs:string" name="Rue" />
			<xs:element type="xs:string" name="Code" />
			<xs:element type="xs:string" name="Ville" />
		</xs:all>
	</xs:complexType>


	<xs:element name="InscriptionDetails" type="exem:InscriptionDetailsType"
		xmlns:exem="http://iaws/ws/contractfirst/covoiturage" />

	<xs:complexType name="InscriptionDetailsType">
		<xs:all>
			<xs:element type="xs:string" name="Code" />
		</xs:all>
	</xs:complexType>
	
</xs:schema>


